@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage;
@inject Umbraco.Cms.Web.Common.UmbracoHelper UmbracoHelper
@{
    var rootContents = UmbracoHelper.ContentAtRoot();
    var settings = rootContents?.FirstOrDefault(x => x.ContentType.Alias == "settingsPage") ?? null;
    var officeLocation = settings?.Value<string>("officeLocation") ?? "Office location not set";
    var siteName = settings?.Value<string>("siteName") ?? "";
    var siteLogotype = settings?.Value<IPublishedContent>("siteLogotype")?.Url() ?? "Cant find image";

}

<header >
    <div class="header-top-row">
        <div class="container header-top-container d-flex justify-content-between align-items-center"> 
            <p class="me-3">@officeLocation</p>
            <div class="d-flex">
                <a href="#" class="social-icon">
                    <i class="fab fa-facebook"></i>
                </a>
                <a href="#" class="social-icon">
                    <i class="fab fa-twitter"></i>
                </a>
                <a href="#" class="social-icon">
                    <i class="fab fa-instagram"></i>
                </a>
            </div>
        </div>
    </div>
    <div class="header-bottom">
        <div class="container header-bottom-container d-flex justify-content-between align-items-center">
            <a class="logotype" href="/"><img src= "@siteLogotype" alt="@siteName"></a>
            <div id="navbar">
                <nav class="navbar">
                    <ul class="nav d-flex">
                        <li class="nav-item">
                            <a class="nav-link" href="/aboutpage">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#services">Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#projects">Projects</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#contact">Contact</a>
                        </li>
                    </ul>
                </nav>
            </div>
        <div class="text-end">
            <a href="#" class="search-icon">
              <i class="fas fa-search"></i> 
            </a>
        </div>
     </div>
    </div>
</header>